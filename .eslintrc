{
  "extends": ["airbnb", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "es5",
        "printWidth": 100,
        "semi": false,
        "parser": "babylon"
      }
    ]
  },
  "globals": {
    "window": true
  },
  "plugins": ["prettier"]
}
